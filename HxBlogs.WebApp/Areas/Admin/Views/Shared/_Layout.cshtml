<!DOCTYPE html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - 海·星の博客</title>
    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/font")
    @Styles.Render("~/Content/app/css/login.css")
    <link href="~/Content/app/css/hx-loading.css" rel="stylesheet" />
    @RenderSection("styles", required: false)
</head>
<body class="login">
    <div class="container body-content ">
        <div class="logo">
            <a href="/">
                <img src="~/Content/Images/head/logo.png" />
            </a>
        </div>
        @RenderBody()
        <div class="copyright">
            @DateTime.Now.Year &copy; HxBlogs by JIE.Tong
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/unobtrusive")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/load")
    @RenderSection("scripts", required: false)
    <script>

    </script>
</body>
</html>